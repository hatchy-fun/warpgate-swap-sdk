{
  "name": "warpgate-swap-sdk",
  "version": "1.0.1",
  "description": "Warpgate Swap SDK for Movement blockchain",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prepare": "npm run clean && npm run build",
    "prepublishOnly": "npm test && npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hatchy-fun/warpgate-swap-sdk"
  },
  "bugs": {
    "url": "https://github.com/hatchy-fun/warpgate-swap-sdk/issues"
  },
  "homepage": "https://github.com/hatchy-fun/warpgate-swap-sdk#readme",
  "keywords": [
    "warpgate",
    "swap",
    "sdk",
    "aptos",
    "movement",
    "dex"
  ],
  "author": "WarpGate",
  "license": "MIT",
  "devDependencies": {
    "@types/big.js": "^6.1.6",
    "@types/node": "^18.0.0",
    "typescript": "^4.9.0",
    "vitest": "^3.0.4",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0"
  },
  "dependencies": {
    "@aptos-labs/ts-sdk": "^1.26.0",
    "big.js": "^6.2.1",
    "decimal.js-light": "^2.5.1",
    "tiny-invariant": "^1.3.1",
    "toformat": "^2.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "@aptos-labs/ts-sdk": "^1.26.0"
  }
